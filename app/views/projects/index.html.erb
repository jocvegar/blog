<h1 id="page_title">My Projects</h1>

<div id= "post_wrapper" class="skinny_wrapper wrapper_padding">
	<% @projects.each do |project| %>
	<div class="post">
		<p class="date"><%= project.created_at.strftime('%A, %d %b %Y') %></p>
		<h2><a href="#"><%= link_to project.title, project %></a></h2>
		<hr>
	</div>
	<% end %>
</div>
